{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1",
      "runtime": "nodejs20",
      "memory": "1GiB",
      "timeoutSeconds": 60,
      "secretEnv": [
        "IMAGE_GEN_API_KEY"
      ]
    },
    "rewrites": [
      {
        "source": "**",
        "function": "ssracoof8e92d"
      }
    ]
  },
  "database": {
    "rules": "database.rules.json"
  },
  "apphosting": [
    {
      "backendId": "acoof-site",
      "rootDir": ".",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "functions"
      ]
    }
  ]
}
